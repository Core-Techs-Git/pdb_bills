{
  "name": "@core-techs-git/pdb_bills",
  "version": "1.0.1",
  "description": "A Bills requester for pdb apps.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analysis": "npm-run-all analysis:*",
    "analysis:circuit": "madge --circular src",
    "analysis:dependency": "check-audit",
    "analysis:lint": "eslint --ext .ts --color ./src",
    "analysis:style": "prettier --check src/**/*.ts",
    "build": "npm run clean && tsc && tspath -f",
    "clean": "rimraf dist",
    "dev": "npm run clean && nodemon ./bin/dev.ts",
    "doc": "typedoc --tsconfig",
    "start": "npm run dev",
    "test": "npm run clean && jest --runInBand --silent --colors"
  },
  "keywords": [
    "pdb",
    "bills",
    "docapost"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-techs-git/pdb_bills.git"
  },
  "author": "Adimeo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Core-Techs-Git/pdb_bills/issues"
  },
  "homepage": "https://github.com/Core-Techs-Git/pdb_bills#readme",
  "engines": {
    "node": "10.15.3",
    "npm": ">=6.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@core-techs-git/pdb_requester": "git+https://git@github.com/Core-Techs-Git/pdb_requester.git#1.1.1",
    "config": "^3.3.0",
    "inversify": "^5.0.1",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.13",
    "validator": "^12.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/validator": "^12.0.1",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "madge": "^3.7.0",
    "nodemon": "^2.0.2",
    "npm-audit-resolver": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tspath": "^1.3.7",
    "typedoc": "^0.16.11",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typescript": "^3.8.3"
  }
}
