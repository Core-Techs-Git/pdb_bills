{
  "name": "@core-techs-git/pdb_bills",
  "version": "1.3.0",
  "description": "A Bills requester for pdb apps.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analysis": "npm-run-all analysis:*",
    "analysis:circuit": "madge --circular src",
    "analysis:dependency": "check-audit",
    "analysis:lint": "eslint --ext .ts --color ./src",
    "analysis:style": "prettier --check src/**/*.ts",
    "build": "npm run clean && ttsc",
    "clean": "rimraf dist",
    "dev": "nodemon ./bin/dev.ts",
    "doc": "typedoc --tsconfig",
    "release": "bin/release",
    "start": "npm run dev",
    "test": "jest --runInBand --silent --colors"
  },
  "keywords": [
    "pdb",
    "bills",
    "docapost"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-techs-git/pdb_bills.git"
  },
  "author": "Adimeo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Core-Techs-Git/pdb_bills/issues"
  },
  "homepage": "https://github.com/Core-Techs-Git/pdb_bills#readme",
  "engines": {
    "node": "10.15.3",
    "npm": ">=6.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@core-techs-git/pdb_requester": "git+https://git@github.com/Core-Techs-Git/pdb_requester.git#1.2.3",
    "config": "3.3.0",
    "inversify": "5.0.1",
    "moment": "2.27.0",
    "reflect-metadata": "0.1.13",
    "validator": "13.1.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/validator": "13.1.0",
    "@types/xml2js": "0.4.5",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "@zerollup/ts-transform-paths": "1.7.18",
    "auto-changelog": "2.2.0",
    "body-parser": "1.19.0",
    "concurrently": "5.3.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "express": "4.17.1",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "jest-junit": "11.1.0",
    "madge": "3.9.2",
    "nodemon": "2.0.4",
    "npm-audit-resolver": "2.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "semver": "7.3.2",
    "ts-jest": "26.2.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "1.5.10",
    "typedoc": "0.18.0",
    "typedoc-plugin-external-module-name": "4.0.3",
    "typedoc-plugin-markdown": "2.4.1",
    "typescript": "3.9.7"
  }
}
